{\n  "version": 2,\n  "buildCommand": "npm run build",\n  "installCommand": "npm install",\n  "functions": {\n    "api/world.js": {\n      "maxDuration": 60,\n      "memory": 1024\n    }\n  },\n  "routes": [\n    {\n      "src": "/api/world",\n      "methods": ["POST", "OPTIONS"],\n      "dest": "/api/world.js"\n    },\n    {\n      "src": "/api/world/([^/]+)/tick",\n      "methods": ["POST"],\n      "dest": "/api/world.js?sessionId=$1"\n    },\n    {\n      "src": "/api/world/([^/]+)/status",\n      "methods": ["GET"],\n      "dest": "/api/world.js?sessionId=$1&action=status"\n    },\n    {\n      "src": "/",\n      "dest": "/public/index.html"\n    },\n    {\n      "handle": "filesystem"\n    },\n    {\n      "src": "/(.*)",\n      "dest": "/public/$1"\n    }\n  ]\n}